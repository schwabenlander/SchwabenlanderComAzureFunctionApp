{
    "version": "2.0",
    "logging": {
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Request"
            },
            "enableLiveMetricsFilters": true
        },
        "extensions": {
            "serviceBus": {
                "maxConcurrentCalls": 1,
                "maxMessageBatchSize": 1
            }
        }
    }
}